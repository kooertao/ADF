{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Log4Net": {
      "Name": "ADF.App"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "LogAOP": {
      "Enabled": true
    },
    "Date": "2020-4-24"
  },
  "Middleware": {
    "RequestResponseLog": {
      "Enabled": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=ADFDb;Trusted_Connection=True;"
  },
  "Audience": {
    "Secret": "sdfsdfsrty45634kkhllghtdgdfss345t678fs", //不要太短，16位+
    "Issuer": "ADF.Core",
    "Audience": "wr"
  },
  "Startup": {
    "IdentityServer4": {
      "Enabled": true, // 这里默认是false，表示使用jwt，如果设置为true，则表示系统使用Ids4模式
      "AuthorizationUrl": "http://localhost:5004", // 认证中心域名
      "ApiName": "ADF.CoreApi" // 资源服务器
    }
  }

}
